# Rumus Persegi Sisi * Sisi

sisi1= 20
sisi2= 20
luas_p =sisi1 * sisi2
print("luas pesegi adalah", luas_p)

# rumus persegi panjang panjang * lebar

panjang= 10
lebar= 20
luas_pp = panjang * lebar

print("Luas persegi panjag adalah",luas_pp)

# rumus trapesium ½ * (a+b) * tinggi
a = 6
B = 10
t = 8
luas_t= (a+B) * t /2
print("Luas trapesium adalah", luas_t)

# rumus segitiga 1/2 a * t

a = 5
t = 8
luas_t = a * t /2
print("luas segitiga adalah", luas_t)


# rumus jajar genjang a * t

a= 15
t = 7
luas_jg = a * t
print("luas jajar genjang adalah", luas_jg)

# luas lingkaran π * r * r * l

phi = 3.14
r =15
r = 10
l =15
luas_l = phi * t * t * l
print("luas lingkaran adalah", luas_l)

